        };

        var trace3 = {
            x: rt_chunk[2],
            y: indiff_chunk[2],
            mode: 'markers',
            type: 'scatter',
            name: 'Trial 3',
            marker: { size: 12 }
        };

        var trace4 = {
            x: rt_chunk[3],
            y: indiff_chunk[3],
            mode: 'markers',
            type: 'scatter',
            name: 'Trial 4',
            marker: { size: 12 }
        };

        var trace5 = {
            x: rt_chunk[4],
            y: indiff_chunk[4],
            mode: 'markers',
            type: 'scatter',
            name: 'Trial 5',
            marker: { size: 12 }
        };

        var data = [trace1, trace2, trace3, trace4, trace5];

        var layout = {
            xaxis: {},
            yaxis: {},
            title: 'Delay Discount Data for Subject: ' + subject_id
        };

        Plotly.newPlot('user2', data, layout);
    </script>
    <script src="../frontend/assets/js/jquery.min.js"></script>
